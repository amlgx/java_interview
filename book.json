{
    "title": "JavaChat",
    "author": "amlgx",
    "description": "java chat",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "amlgx": "https://i.amlgx.com/"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "github",
        "github-buttons",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/amlgx/javachat"
        },
        "github-buttons": {
            "buttons": [{
                "user": "amlgx",
                "repo": "javachat",
                "type": "star",
                "size": "small",
                "count": true
                }
            ]
        },
        "favicon":{
            "shortcut": "./source/images/favicon.jpg",
            "bookmark": "./source/images/favicon.jpg"
        }
    }
}
